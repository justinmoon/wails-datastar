package views


templ Signals(count2 int) {
    <div id="signalsContainer" 
         data-signals-count2="0">
        <p data-text="$count2"></p>
        <button data-on-click="@ipc('IncrementSignals')">＋</button>
        <button data-on-click="@ipc('DecrementSignals')">－</button>
        <button data-on-click="@ipc('RemoveSignalsUI')">Remove fragment</button>
    </div>
}
